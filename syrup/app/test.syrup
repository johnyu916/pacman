    print(["test_eyes", eyes.velocity, eyes.eye_state])

    eyes.position = [45, 0, 213]
    move_eyes_velocity(navigation, delta, eyes)
    print(["test_eyes", eyes.velocity, eyes.eye_state])

    eyes.position = [93, 0, 165]
    move_eyes_velocity(navigation, delta, eyes)
    print(["test_eyes", eyes.velocity, eyes.eye_state])

    eyes.position = [105, 0, 165]
    move_eyes_velocity(navigation, delta, eyes)
    print(["test_eyes", eyes.velocity, eyes.eye_state])

    eyes.position = [105, 0, 141]
